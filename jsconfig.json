{
	"extends": "./.denoTypes/tsconfig.json",
	"compilerOptions": {
		"lib": ["dom", "dom.iterable", "esnext", "WebWorker"],
		"target": "esnext",
		"checkJs": true,
		"module": "esnext",
		"useDefineForClassFields": true,
		"strict": true,
		"exactOptionalPropertyTypes": true
	},
	"include": [
		"scripts/**/*.js",
		"gameServer/src/**/*.js",
		"servermanager/src/**/*.js",
		"adminpanel/src/**/*.js",
		"shared/**/*.js"
	]
}
